{
    "version": 2,
    "builds": [
      {
        "src": "src/next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/src/pages/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/src/pages/$1"
      }
    ]
  }